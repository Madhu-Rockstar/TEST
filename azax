                            }
                           
                        }
                        else {
                            $("#lblPharmacyTotalClaims").html("N/A");
                            $("#lblPharmacyTotalPatients").html("N/A");
                            $("#lblPharmacyTotalCost").html("N/A");
                            $("#lblPharmacyBenchmarkPMPY").html("N/A");
                            $("#lblPharmacyActualPMPY").html("N/A");
                        }

                        //Graph       
                        var Values = new Array();
