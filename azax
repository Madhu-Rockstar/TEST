                        if (this.value.length == 0) {
                            var dtable = $("#tblUtilization").dataTable().api();
                            dtable.search("").draw();
                        }

                    });
                },
                aoColumns: [
                    { "data": "HICN", "title": "HICN", sClass: "text-center" },
