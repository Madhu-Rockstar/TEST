                    if (this.value.length > 3) {
                        var dtable = $("#tblClinicalDistribution").dataTable().api();
                        dtable.search(this.value).draw();
                    }

                    if (this.value.length == 0) {
                        var dtable = $("#tblClinicalDistribution").dataTable().api();
                        dtable.search("").draw();
                    }

                });

            },
            order: [[15, "desc"]],
