                        if (this.value.length > 3) {
                            var dtable = $("#tblpvbr").dataTable().api();
                            dtable.search(this.value).draw();
                        }

                        if (this.value.length == 0) {
                            var dtable = $("#tblpvbr").dataTable().api();
